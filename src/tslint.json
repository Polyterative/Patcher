{
  "extends": "../tslint.json",
  "rules":   {
    "directive-selector": [
      true,
      "attribute",
      "app",
      "camelCase"
    ],
    "component-selector": [
      false,
      "element",
      "app",
      "kebab-case"
    ],
    "import-spacing":     [
      false
    ],
    "no-trailing-whitespace": [
      false
    ]
  }
}
