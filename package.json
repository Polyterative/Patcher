{
  "name":            "patcher",
  "version":         "2.6.1",
  "scripts":         {
    "start":         "ng serve --hmr ",
    "build":         "angular-build-info && ng build --configuration production",
    "deploy":        "angular-build-info && ng build --configuration production && firebase deploy",
    "updateMinors":  "npm update --save/--save-dev",
    "release:patch": "standard-version --release-as patch && angular-build-info && git push --follow-tags origin master",
    "release:minor": "standard-version --release-as minor && angular-build-info && git push --follow-tags origin master",
    "release:major": "standard-version --release-as major && angular-build-info && git push --follow-tags origin master",
    "bundle-report": "ng build --configuration=production --stats-json && webpack-bundle-analyzer dist/Patcher/browser/stats.json",
    "test":          "ng test",
    "lint":          "ng lint",
    "e2e":           "ng e2e"
  },
  "private":         true,
  "dependencies":    {
    "@angular/animations":               "^12.0.0",
    "@angular/cdk":                      "^12.0.0",
    "@angular/common":                   "^12.0.0",
    "@angular/compiler":                 "^12.0.0",
    "@angular/core":                     "^12.0.0",
    "@angular/fire":                     "6.1.4",
    "@angular/flex-layout":              "^11.0.0-beta.33",
    "@angular/forms":                    "^12.0.0",
    "@angular/material":                 "^12.0.0",
    "@angular/platform-browser":         "^12.0.0",
    "@angular/platform-browser-dynamic": "^12.0.0",
    "@angular/router":                   "^12.0.0",
    "@ngx-pwa/local-storage":            "^11.1.0",
    "@supabase/supabase-js":             "^1.11.13",
    "angular-html-parser":               "^1.8.0",
    "angular-tslint-rules":              "^1.20.4",
    "core-js":                           "^3.12.1",
    "firebase":                          "^7.14.5",
    "luxon":                             "^1.26.0",
    "luxon-angular":                     "^3.1.0",
    "ngx-timeago":                       "^2.0.0",
    "rxjs":                              "^6.6.3",
    "smart-picture":                     "^0.1.0",
    "time-ago-pipe":                     "^1.3.2",
    "tslib":                             "^2.0.0",
    "zone.js":                           "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular":    "^12.0.0",
    "@angular/cli":                     "^12.0.0",
    "@angular/compiler-cli":            "^12.0.0",
    "@angular/language-service":        "^12.0.0",
    "@types/jasmine":                   "^3.7.2",
    "@types/jasminewd2":                "^2.0.9",
    "@types/luxon":                     "^1.26.5",
    "@types/node":                      "^12.20.13",
    "angular-build-info":               "^2.0.1",
    "codelyzer":                        "^5.1.2",
    "jasmine-core":                     "^3.4.0",
    "jasmine-spec-reporter":            "^4.2.1",
    "karma":                            "^6.3.2",
    "karma-chrome-launcher":            "^3.1.0",
    "karma-coverage-istanbul-reporter": "^2.1.0",
    "karma-jasmine":                    "^2.0.1",
    "karma-jasmine-html-reporter":      "^1.6.0",
    "protractor":                       "^7.0.0",
    "release-it":                       "^14.6.2",
    "ts-node":                          "^8.4.1",
    "tslint":                           "^6.1.3",
    "typescript":                       "4.2.4"
  }
}
