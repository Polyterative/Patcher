{
  "name": "patcher",
  "version": "4.4.1",
  "scripts": {
    "start": "angular-build-info && ng serve --port 5556",
    "startProd": "ng serve --configuration production --hmr ",
    "build": "angular-build-info && ng build --configuration production",
    "deploy": "angular-build-info && ng build --configuration production && firebase deploy",
    "copyChangelog": "copyfiles changelog.md assets/md/",
    "updateMinors": "npm update --save/--save-dev",
    "updateAll": "ncu -u",
    "ci": "",
    "updateBackendTypes": "npx supabase gen types typescript --project-id sozmatmywjpstwidzlss --schema public > src/backend/database.types.ts",
    "testRelease:patch": "standard-version  --release-as patch --dry-run && git push --follow-tags origin production",
    "release:patch": "standard-version  --release-as patch --commit-all  && git push --follow-tags origin production",
    "release:minor": "standard-version  --release-as minor --commit-all  && git push --follow-tags origin production",
    "release:major": "standard-version  --release-as major --commit-all  && git push --follow-tags origin production",
    "bundle-report": "ng build --configuration=production --stats-json && webpack-bundle-analyzer dist/Patcher/browser/stats.json",
    "test": "ng test --browsers=ChromeHeadless",
    "test-headless": "ng test --browsers=ChromeHeadless --watch=false",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "standard-version": {
    "scripts": {
      "postchangelog": "copyfiles changelog.md src/assets/md/",
      "precommit": "git add src/assets/md/changelog.md"
    }
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^14.0.4",
    "@angular/cdk": "^14.0.4",
    "@angular/cdk-experimental": "^14.0.4",
    "@angular/common": "^14.0.4",
    "@angular/compiler": "^14.0.4",
    "@angular/core": "^14.0.4",
    "@angular/flex-layout": "^14.0.0-beta.40",
    "@angular/forms": "^14.0.4",
    "@angular/material": "^14.0.4",
    "@angular/platform-browser": "^14.0.4",
    "@angular/platform-browser-dynamic": "^14.0.4",
    "@angular/router": "^14.0.4",
    "@ngx-pwa/local-storage": "^13.0.2",
    "@sentry/angular": "^7.5.0",
    "@sentry/tracing": "^7.5.0",
    "@supabase/supabase-js": "^2.39.3",
    "angular-animations": "^0.11.0",
    "angular-html-parser": "^1.8.0",
    "core-js": "^3.23.3",
    "eslint": "^8.19.0",
    "graphology": "^0.24.1",
    "graphology-generators": "^0.11.2",
    "graphology-layout": "^0.5.0",
    "graphology-layout-force": "^0.2.4",
    "graphology-layout-forceatlas2": "^0.8.2",
    "graphology-types": "^0.24.3",
    "dom-to-image": "^2.6.0",
    "lodash": "^4.17.21",
    "lottie-web": "^5.9.1",
    "luxon": "^2.3.1",
    "luxon-angular": "^5.0.0",
    "ngx-dropzone": "3.1.0",
    "ngx-lottie": "^8.2.0",
    "ngx-markdown": "^14.0.1",
    "ngx-pipes": "^3.0.0",
    "ngx-seo": "^5.3.0",
    "ngx-timeago": "^2.0.0",
    "rxjs": "^7.5.5",
    "sigma": "^2.2.0",
    "tslib": "^2.3.1",
    "zone.js": "~0.11.6"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.0.4",
    "@angular-eslint/builder": "^14.0.0",
