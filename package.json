{
  "name": "patcher",
  "version": "2.17.7",
  "scripts": {
    "start": "angular-build-info && ng serve --hmr --port 5556",
    "startProd": "ng serve --configuration production --hmr ",
    "build": "angular-build-info && ng build --configuration production",
    "deploy": "angular-build-info && ng build --configuration production && firebase deploy",
    "updateMinors": "npm update --save/--save-dev",
    "ci": "",
    "release:patch": "standard-version --release-as patch && git push --follow-tags origin master",
    "release:minor": "standard-version --release-as minor && git push --follow-tags origin master",
    "release:major": "standard-version --release-as major && git push --follow-tags origin master",
    "bundle-report": "ng build --configuration=production --stats-json && webpack-bundle-analyzer dist/Patcher/browser/stats.json",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^12.2.5",
    "@angular/cdk": "^12.2.5",
    "@angular/cdk-experimental": "^12.2.5",
    "@angular/common": "^12.2.5",
    "@angular/compiler": "^12.2.5",
    "@angular/core": "^12.2.5",
    "@angular/fire": "6.1.5",
    "@angular/flex-layout": "^12.0.0-beta.34",
    "@angular/forms": "^12.2.5",
    "@angular/material": "^12.2.5",
    "@angular/platform-browser": "^12.2.5",
    "@angular/platform-browser-dynamic": "^12.2.5",
    "@angular/router": "^12.2.5",
    "@ngneat/until-destroy": "^8.1.4",
    "@ngx-pwa/local-storage": "^12.1.0",
    "@sentry/angular": "^6.12.0",
    "@sentry/tracing": "^6.12.0",
    "@supabase/supabase-js": "^1.22.6",
    "angular-html-parser": "^1.8.0",
    "angular-tslint-rules": "^1.20.4",
    "core-js": "^3.17.3",
    "firebase": "^8.10.0",
    "luxon": "^1.28.0",
    "luxon-angular": "^3.1.0",
    "ngx-timeago": "^2.0.0",
    "rxjs": "^6.6.3",
    "tslib": "^2.3.1",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^12.2.5",
    "@angular/cli": "^12.2.5",
    "@angular/compiler-cli": "^12.2.5",
    "@angular/language-service": "^12.2.5",
    "@types/jasmine": "^3.9.0",
    "@types/jasminewd2": "^2.0.10",
    "@types/luxon": "^1.27.1",
    "@types/node": "^15.14.9",
    "angular-build-info": "^2.0.1",
    "codelyzer": "^5.1.2",
    "jasmine-core": "^3.9.0",
    "jasmine-spec-reporter": "^7.0.0",
    "karma": "^6.3.4",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage-istanbul-reporter": "^3.0.3",
    "karma-jasmine": "^4.0.1",
    "karma-jasmine-html-reporter": "^1.7.0",
    "protractor": "^7.0.0",
    "standard-version": "^9.3.1",
    "ts-node": "^8.4.1",
    "tslint": "^6.1.3",
    "typescript": "4.2.4"
  }
}
