{
  "name": "patcher",
  "version": "3.0.12",
  "scripts": {
    "start":         "angular-build-info && ng serve --hmr --port 5556",
    "startProd":     "ng serve --configuration production --hmr ",
    "build":         "angular-build-info && ng build --configuration production",
    "deploy":        "angular-build-info && ng build --configuration production && firebase deploy",
    "copyChangelog": "copyfiles changelog.md assets/md/",
    "updateMinors":  "npm update --save/--save-dev",
    "updateAll":     "ncu -u",
    "ci":            "",
    "release:patch": "standard-version --release-as patch && git push --follow-tags origin production",
    "release:minor": "standard-version --release-as minor && git push --follow-tags origin production",
    "release:major": "standard-version --release-as major && git push --follow-tags origin production",
    "bundle-report": "ng build --configuration=production --stats-json && webpack-bundle-analyzer dist/Patcher/browser/stats.json",
    "test":          "ng test",
    "lint":          "ng lint",
    "e2e":           "ng e2e"
  },
  "standard-version": {
    "scripts": {
      "postchangelog": "copyfiles changelog.md assets/md/"
    }
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^13.1.2",
    "@angular/cdk": "^13.1.2",
    "@angular/cdk-experimental": "^13.1.2",
    "@angular/common": "^13.1.2",
    "@angular/compiler": "^13.1.2",
    "@angular/core": "^13.1.2",
    "@angular/fire": "7.2.0",
    "@angular/flex-layout": "^13.0.0-beta.36",
    "@angular/forms": "^13.1.2",
    "@angular/material": "^13.1.2",
    "@angular/platform-browser": "^13.1.2",
    "@angular/platform-browser-dynamic": "^13.1.2",
    "@angular/router": "^13.1.2",
    "@ngx-pwa/local-storage": "^13.0.2",
    "@sentry/angular": "^6.16.1",
    "@sentry/tracing": "^6.16.1",
    "@supabase/supabase-js": "^1.29.2",
    "angular-html-parser": "^1.8.0",
    "core-js": "^3.20.3",
    "firebase": "^9.6.3",
    "graphology": "^0.23.2",
    "graphology-generators": "^0.11.2",
    "graphology-layout": "^0.5.0",
    "graphology-layout-force": "^0.2.4",
    "graphology-layout-forceatlas2": "^0.8.1",
    "graphology-types": "^0.23.0",
    "lottie-web": "^5.8.1",
    "luxon": "^2.3.0",
    "luxon-angular": "^5.0.0",
    "ngx-lottie": "^8.0.1",
    "ngx-timeago": "^2.0.0",
    "rxjs": "^7.5.2",
    "sigma": "^2.1.3",
    "tslib": "^2.3.1",
    "tslint": "^6.1.3",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^13.1.3",
    "@angular/cli": "^13.1.3",
    "@angular/compiler-cli": "^13.1.2",
    "@angular/language-service": "^13.1.2",
    "@types/jasmine": "^3.10.3",
    "@types/jasminewd2": "^2.0.10",
    "@types/luxon": "^2.0.9",
    "@types/node": "^17.0.10",
    "angular-build-info": "^2.0.1",
    "angular-tslint-rules": "^1.20.4",
    "codelyzer": "^6.0.2",
    "copyfiles": "^2.4.1",
    "jasmine-core": "^4.0.0",
    "jasmine-spec-reporter": "^7.0.0",
    "karma": "^6.3.11",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage-istanbul-reporter": "^3.0.3",
    "karma-jasmine": "^4.0.1",
    "karma-jasmine-html-reporter": "^1.7.0",
    "protractor": "^7.0.0",
    "standard-version": "^9.3.2",
    "ts-node": "^10.4.0",
    "typescript": "4.5.4"
  }
}
