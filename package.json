{
  "name": "patcher",
  "version": "4.0.0",
  "scripts": {
    "start": "angular-build-info && ng serve --hmr --port 5556",
    "startProd": "ng serve --configuration production --hmr ",
    "build": "angular-build-info && ng build --configuration production",
    "deploy": "angular-build-info && ng build --configuration production && firebase deploy",
    "copyChangelog": "copyfiles changelog.md assets/md/",
    "updateMinors": "npm update --save/--save-dev",
    "updateAll": "ncu -u",
    "ci": "",
    "testRelease:patch": "standard-version  --release-as patch --dry-run && git push --follow-tags origin production",
    "release:patch": "standard-version  --release-as patch --commit-all  && git push --follow-tags origin production",
    "release:minor": "standard-version  --release-as minor --commit-all  && git push --follow-tags origin production",
    "release:major": "standard-version  --release-as major --commit-all  && git push --follow-tags origin production",
    "bundle-report": "ng build --configuration=production --stats-json && webpack-bundle-analyzer dist/Patcher/browser/stats.json",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "standard-version": {
    "scripts": {
      "postchangelog": "copyfiles changelog.md src/assets/md/",
      "precommit": "git add src/assets/md/changelog.md"
    }
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^13.2.5",
    "@angular/cdk": "^13.2.5",
    "@angular/cdk-experimental": "^13.2.5",
    "@angular/common": "^13.2.5",
    "@angular/compiler": "^13.2.5",
    "@angular/core": "^13.2.5",
    "@angular/flex-layout": "^13.0.0-beta.38",
    "@angular/forms": "^13.2.5",
    "@angular/material": "^13.2.5",
    "@angular/platform-browser": "^13.2.5",
    "@angular/platform-browser-dynamic": "^13.2.5",
    "@angular/router": "^13.2.5",
    "@ngx-pwa/local-storage": "^13.0.2",
    "@sentry/angular": "^6.18.1",
    "@sentry/tracing": "^6.18.1",
    "@supabase/supabase-js": "^1.30.7",
    "angular-animations": "^0.11.0",
    "angular-html-parser": "^1.8.0",
    "core-js": "^3.21.1",
    "graphology": "^0.24.1",
    "graphology-generators": "^0.11.2",
    "graphology-layout": "^0.5.0",
    "graphology-layout-force": "^0.2.4",
    "graphology-layout-forceatlas2": "^0.8.2",
    "graphology-types": "^0.24.3",
    "lodash": "^4.17.21",
    "lottie-web": "^5.9.1",
    "luxon": "^2.3.1",
    "luxon-angular": "^5.0.0",
    "ngx-lottie": "^8.0.1",
    "ngx-markdown": "^13.1.0",
    "ngx-seo": "^5.3.0",
    "ngx-timeago": "^2.0.0",
    "rxjs": "^7.5.4",
    "sigma": "^2.2.0",
    "tslib": "^2.3.1",
    "tslint": "^6.1.3",
    "zone.js": "~0.11.5"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^13.2.5",
    "@angular/cli": "^13.2.5",
    "@angular/compiler-cli": "^13.2.5",
    "@angular/language-service": "^13.2.5",
    "@types/jasmine": "^3.10.3",
    "@types/jasminewd2": "^2.0.10",
    "@types/lodash": "^4.14.179",
    "@types/luxon": "^2.3.0",
    "@types/node": "^17.0.21",
    "angular-build-info": "^2.0.1",
    "angular-tslint-rules": "^1.20.4",
    "codelyzer": "^6.0.2",
    "copyfiles": "^2.4.1",
    "jasmine-core": "^4.0.1",
    "jasmine-spec-reporter": "^7.0.0",
    "karma": "^6.3.17",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage-istanbul-reporter": "^3.0.3",
    "karma-jasmine": "^4.0.1",
    "karma-jasmine-html-reporter": "^1.7.0",
    "protractor": "^7.0.0",
    "standard-version": "^9.3.2",
    "ts-node": "^10.7.0",
    "typescript": "4.5.5"
  }
}
